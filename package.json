{
  "name": "demo-graphql-api",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.12.0"
  },
  "exports": "./dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon --watch './**/*.ts' --exec 'NODE_ENV=DEVELOP ts-node' src/app.ts",
    "build": "rm -rf dist && tsc",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "prettier": "prettier --config-precedence prefer-file -c '**/*.{tsx,ts,js,jsx,json,graphql,scss,yml}'",
    "prettier:fix": "prettier --write --config-precedence prefer-file -c '**/*.{tsx,ts,js,jsx,json,graphql,scss,yml}'",
    "lint": "npx eslint 'src/**/*.{ts,graphql}' --cache",
    "lint:fix": "eslint 'src/**/*.{ts,graphql}' --fix",
    "migration:create": "ts-node src/migrations/scripts/create.ts",
    "migration:up": "ts-node src/migrations/scripts/up.ts",
    "migration:down": "ts-node src/migrations/scripts/down.ts",
    "seed:up": "ts-node src/testing/seed/scripts/seed.ts"
  },
  "dependencies": {
    "@debugr/apollo": "^3.0.0-rc.9",
    "@debugr/console": "^3.0.0-rc.10",
    "@debugr/core": "^3.0.0-rc.12",
    "@debugr/elastic": "^3.0.0-rc.10",
    "@debugr/express": "^3.0.0-rc.7",
    "@mikro-orm/cli": "^6.1.9",
    "@mikro-orm/core": "^6.1.9",
    "@mikro-orm/migrations": "^6.1.9",
    "@mikro-orm/reflection": "^6.1.9",
    "@mikro-orm/seeder": "^6.1.9",
    "@mikro-orm/sqlite": "^6.1.9",
    "apollo-server-core": "3.13.0",
    "apollo-server-express": "3.13.0",
    "body-parser": "1.20.2",
    "class-validator": "0.14.1",
    "cors": "2.8.5",
    "dayjs": "^1.11.10",
    "dotenv-cli": "7.4.1",
    "dotenv-safe": "9.1.0",
    "express": "4.18.3",
    "graphql": "^16.8.1",
    "graphql-depth-limit": "1.1.0",
    "graphql-fields-to-relations": "^1.0.5",
    "graphql-middleware": "^6.1.35",
    "graphql-request": "6.1.0",
    "graphql-scalars": "^1.22.5",
    "helmet": "7.1.0",
    "lodash": "4.17.21",
    "reflect-metadata": "^0.2.1",
    "type-graphql": "^2.0.0-beta.6",
    "typedi": "0.10.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@graphql-codegen/add": "5.0.2",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript": "4.0.6",
    "@graphql-codegen/typescript-document-nodes": "4.0.6",
    "@graphql-codegen/typescript-graphql-request": "6.2.0",
    "@graphql-codegen/typescript-operations": "4.2.0",
    "@graphql-codegen/typescript-resolvers": "4.0.6",
    "@types/body-parser": "1.19.5",
    "@types/cors": "2.8.17",
    "@types/dotenv-safe": "8.1.6",
    "@types/express": "4.17.21",
    "@types/graphql-depth-limit": "1.1.6",
    "@types/lodash": "4.14.202",
    "@types/node": "20.11.25",
    "@types/prettier": "2.7.3",
    "@types/uuid": "^9.0.8",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "nodemon": "3.1.0",
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.1.1"
  }
}
